# Set default values for all following accounts.
defaults
auth           on
tls            on
tls_trust_file /etc/ssl/certs/ca-certificates.crt
logfile        ~/.msmtp.log

# Gmail
account        gmail
host           smtp.gmail.com
port           587
from           ratier.franck@gmail.com
user           ratier.franck
passwordeval   pass gmail/smtp_pass

# Set a default account
account default : gmail
